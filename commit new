mkdir my-repo
cd my-repo
git init
echo "Initial README file" > README.md
git add README.md
git commit -m "Commit 1"
echo "This line is added after Commit 1" >> README.md
git status
git add README.md
git commit -m "Commit 2"
echo "This line is added after Commit 2" >> README.md
git status
git add README.md
git commit -m "Commit 3"
git log --oneline
